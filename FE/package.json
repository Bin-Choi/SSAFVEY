{
  "name": "fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn vite\" \"npx tailwindcss -i ./src/index.css -o ./src/output.css --watch\"",
    "build": "concurrently \"tsc && vite build\" \"npx tailwindcss -i ./src/index.css -o ./src/output.css\"",
    "preview": "vite preview",
    "ac": "node tools/gitadd.js",
    "head0": "git reset --hard HEAD",
    "head1": "git reset --hard HEAD^",
    "head2": "git reset --hard HEAD^^",
    "head3": "git reset --hard HEAD^^^"
  },
  "dependencies": {
    "concurrently": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.1",
    "vite-plugin-pwa": "^0.14.7"
  }
}
